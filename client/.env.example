# Mulisa Client Environment Configuration
# Copy this to .env.local and fill in your actual values

# Core Server Configuration
VITE_SERVER_URL=http://localhost:3001
VITE_BOT_SERVER_URL=http://localhost:4000

# LiveKit Configuration (Optional - for advanced features)
VITE_LIVEKIT_URL=wss://your-livekit-server-url
VITE_LIVEKIT_API_KEY=your-livekit-api-key
VITE_LIVEKIT_API_SECRET=your-livekit-api-secret
VITE_LIVEKIT_TOKEN_URL=${VITE_SERVER_URL}/api/get-livekit-token

# Development Configuration
VITE_NODE_ENV=development
VITE_DEBUG=true

{
  "version": 2,
  "buildCommand": "npm run install:all && cd client && npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/agent",
      "dest": "/index.html"
    },
    {
      "src": "/customer", 
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
